// Program Menghitung Luas Kebun Persegi Panjang

DECLARE LebarKebun   : INTEGER
DECLARE PanjangKebun : INTEGER
DECLARE LuasKebun    : INTEGER

// Input lebar kebun
OUTPUT "Masukkan Lebar Kebun : "
INPUT LebarKebun

// Input panjang kebun
OUTPUT "Masukkan Panjang Kebun : "
INPUT PanjangKebun

// Hitung luas kebun
LuasKebun <- LebarKebun * PanjangKebun

// Tampilkan hasil
OUTPUT "Luas Kebun Adalah : ", LuasKebun, " mÂ²"
