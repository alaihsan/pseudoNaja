// program: tebak umur.
// siapkan 2 variabel (tahun sekarang dan tahun lahir).
// tipe data variabel, integer.
// siapkan input nilai 
// hitung 2 variabel dan tampilkan

DECLARE tahunSekarang: INTEGER
DECLARE tahunLahir: INTEGER

OUTPUT "Masukkan tahun sekarang:"
INPUT tahunSekarang

OUTPUT "Masukkan tahun lahirmu:"
INPUT tahunLahir

OUTPUT "usia kamu adalah ", tahunSekarang - tahunLahir, "tahun"
